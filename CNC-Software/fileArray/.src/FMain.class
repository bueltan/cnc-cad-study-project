Public Sub Button3_Click()
Dim i As Integer
ListBox1.Clear
ListBox2.Clear
xx.Clear
 yy.Clear
contenidox = File.Load("/home/Denis/Documentos" & "/prueba.Cncx")
 contenidoy = File.Load("/home/Denis/Documentos" & "/prueba.Cncy")
 xx = Split(contenidox, "-")
  yy = Split(contenidoy, "-")

 For i = 0 To xx.Max 
 ListBox1.Add(xx[i])
 Next
   For i = 0 To yy.Max 
   ListBox2.Add(yy[i])
  Next
contenidox = ""
 contenidoy = ""
End

